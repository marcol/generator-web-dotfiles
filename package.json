{
  "name": "generator-web-dotfiles",
  "version": "3.1.0",
  "private": false,
  "description": "Add dofiles for your JavaScript/Web projects. It will setup linting for js, css, markdown, html. Add validation to commits and githooks for testing and linting.",
  "keywords": [
    "CSS",
    "HTML",
    "JavaScript",
    "js",
    "git",
    "ci",
    "quick",
    "simple",
    "web",
    "dotfiles",
    ".eslint",
    ".gitignore",
    ".stylelint",
    "yeoman-generator",
    "yo",
    "generator",
    "yeoman"
  ],
  "scripts": {
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint",
    "lint:md": "remark . --quiet",
    "test": "yarn test:unit",
    "test:unit": "jest",
    "_postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "bugs": "https://github.com/marcol/generator-web-dotfiles/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcol/generator-web-dotfiles.git"
  },
  "license": "ISC",
  "files": [
    "generators"
  ],
  "dependencies": {
    "deep-extend": "^0.6.0",
    "sugar-chalk": "^1.6.7",
    "yeoman-generator": "^5.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "jest": "^26.6.3",
    "listr": "^0.14.3",
    "ora": "^6.0.1",
    "pinst": "^2.1.6",
    "remark-cli": "^10.0.0",
    "remark-lint": "^9.1.0",
    "remark-preset-lint-markdown-style-guide": "^5.1.0",
    "rimraf": "^3.0.2",
    "string-sanitizer": "^2.0.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-environment": "^3.6.0",
    "yeoman-test": "^6.2.0"
  },
  "resolutions": {}
}
