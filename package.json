{
  "name": "generator-web-dotfiles",
  "version": "1.1.3",
  "description": "Yeoman generator with dofiles for your web projects",
  "keywords": [
    "CSS",
    "HTML",
    "JavaScript",
    "Git",
    "simple",
    "web",
    "dotfiles",
    ".eslint",
    ".gitignore",
    ".stylelint",
    "yeoman-generator"
  ],
  "scripts": {
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "eslint .",
    "lint:md": "remark . --quiet",
    "test": "mocha 'test/index.js'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "bugs": "https://github.com/marcol/generator-web-dotfiles/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcol/generator-web-dotfiles.git"
  },
  "license": "ISC",
  "files": [
    "generators"
  ],
  "dependencies": {
    "yeoman-generator": "^4.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "chalk": "^4.1.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "mocha": "^8.0.1",
    "remark-cli": "^8.0.0",
    "remark-lint": "^7.0.1",
    "remark-preset-lint-markdown-style-guide": "^3.0.1",
    "rimraf": "^3.0.2",
    "string-sanitizer": "^1.1.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^3.0.0"
  }
}
